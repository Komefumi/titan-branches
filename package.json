    "prod-build": "yarn run clear-dist && yarn run build-bundle && yarn run correct:build-bundle-linkage",
    "dev-backend-server-start": "ts-node config/server",
    "serve-generated-webpages": "live-server generated-webpages --mount=/dist:./dist --port=3434 --no-browser",
